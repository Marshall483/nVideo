@{
  Layout = "AdminPanelLayout";
}
<div class="p-4">
  <div class="welcome">
    <div class="content rounded-3 p-3">
      @{
        foreach (Catalog_Entity ent in ViewBag.Entities)
        {
          <h3><a class="nav-link" href="~/Catalog/About/@ent.Id">@ent.Name</a></h3>
          <p>@ent.Short_Desc</p>
          <p>Is Awailable: @ent.Awailable</p>
          <h5><a class="nav-link">Edit</a>     <a class="nav-link" href="/AdminPanel/DeleteEntities?Id=@ent.Id">Delete</a></h5>
          <br/>
          <br/>
        } 
      }
      
    </div>
  </div>
</div>

