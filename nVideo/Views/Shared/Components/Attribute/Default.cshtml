@model Dictionary<string, List<Catalog_Value>>

<form method="get" asp-controller="Catalog" asp-action="EntityByFilter">
    @foreach (var pair in Model)
    {
        @pair.Key
        @await Html.PartialAsync("FilterPartial", new AttributeView(pair.Key, pair.Value))
    }
    <input type="submit" value="Search" class="btn">
</form>
